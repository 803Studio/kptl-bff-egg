{
  "exclude": [
    "./node_modules",
    "./test",
    "typings"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "Node",
    "module": "CommonJS",
    "declaration": true,
    //"sourceMap": true,
    "outDir": "./app",
    "rootDir": "./lib/ts",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  }
}
